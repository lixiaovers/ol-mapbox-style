{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "target": "ES5",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "ol": ["node_modules/ol-zhyt/src"],
      "ol-zhyt/*": ["node_modules/ol-zhyt/src/*"]
    },
    "lib": [
      "es2015",
      "dom",
      "webworker"
    ]
  },
  "include": [
    "src/**/*.js",
    "node_modules/ol-zhyt/src/**/*.js"
  ]
}
